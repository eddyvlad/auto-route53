{
  "name": "eddyhidayat-ddns",
  "version": "1.0.0",
  "description": "DDNS implementation for AWS Lambda",
  "main": "dist/index.js",
  "scripts": {
    "start": "esr index.ts",
    "build": "tsc",
    "deploy": "aws lambda update-function-code --function-name eddyhidayat-ddns --zip-file fileb://lambda-deployment.zip",
    "test": "jest",
    "zip": "npm pack && tar -xzf eddyhidayat-ddns-*.tgz && cd package && mv dist/* ./ && rm -rf ./dist && npm install --omit=dev && zip -r ../lambda-deployment.zip . && cd ../ && rm -rf ./package && rm eddyhidayat-ddns-*.tgz"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-route-53": "^3.654.0"
  },
  "files": [
    "dist/",
    "node_modules/",
    "package.json"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "esbuild": "^0.24.0",
    "esbuild-runner": "^2.2.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.2"
  }
}
